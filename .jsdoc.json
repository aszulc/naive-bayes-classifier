{
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(js)$"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8"
  },
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "extensions": [
      "js",
      "es6",
      "jsx"
    ],
    "plugins": "transform-flow-strip-types"
  }
}